rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /tokens/{uid} {
      function isSignedIn() {
        return request.auth != null;
      }

      function isOwner() {
        return uid == request.auth.uid;
      }

      allow read, write: if isSignedIn() && isOwner();
    }
  }
}
